# Definición de la clase A
.DATA
VT_A:
DW getX_A_13
.CODE
main_A_0:
	LOADFP	; Guardo enlace dinámico
	LOADSP	; Inicializo FP
	STOREFP

	RMEM 1	; Reservo espacio para vars locales
	RMEM 1	; Creación CIR
	PUSH 3	; Reservo lugar para variables de instancia y VT
	PUSH simple_malloc
	CALL
	DUP	; Asignación de la VT al CIR creado
	PUSH VT_A
	STOREREF 0	;Guardo referecia a VT
	DUP
	;Generación atributos inline
	PUSH atrInline_A_14
	CALL
	PUSH A_A_15	; Apilo etiqueta del constructor
	CALL
	RMEM 1	; Reservo lugar para el retorno
	SWAP
	DUP
	LOADREF 0
	LOADREF 0	; Cargo la dirección del método
	CALL
	STORE 0
