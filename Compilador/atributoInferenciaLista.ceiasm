.CODE
main_Prueba_0:
	LOADFP	; Guardo enlace dinámico
	LOADSP	; Inicializo FP
	STOREFP

	RMEM 2	; Reservo espacio para vars locales de las declaraciones explícitas
	RMEM 1	; Reservo espacio para var local p
	RMEM 1	; Creación CIR (clase Prueba)
	PUSH 3	; Reservo lugar para variables de instancia y VT (clase Prueba)
	PUSH simple_malloc
	CALL
	DUP	; Asignación de la VT al CIR creado
	PUSH VT_Prueba
	STOREREF 0	;Guardo referecia a VT
	DUP
	PUSH Prueba_Prueba_15	; Apilo etiqueta del constructor Prueba_Prueba_15
	CALL	; Llamo al constuctor Prueba_Prueba_15
	STORE -2
	LOAD -2
	RMEM 1	; Reservo lugar para el retorno
	SWAP
	DUP
	LOADREF 0
	LOADREF 0	; Cargo la dirección del método getX (clase Prueba)
	CALL
	STORE 0
	LOAD -2
	RMEM 1	; Reservo lugar para el retorno
	SWAP
	DUP
	LOADREF 0
